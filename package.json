{
  "name": "wisnuc-drop",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --delay 1.5 src/bin/www",
    "start-debug": "node src/bin/www",
    "debug": "DEBUG=*. node src/bin/www",
    "test": "NODE_ENV=test nodemon src/bin/www",
    "mocha": "NODE_PATH=. NODE_ENV=test nodemon --ignore out --exec mocha --recursive --sort",
    "unit": "npm run mocha",
    "unit-agent": "NODE_PATH=. NODE_ENV=test nodemon --exec mocha --recursive test/agent",
    "debug-socket.io": "DEBUG=socket.io* nodemon ./bin/www",
    "doc": "nodemon --ignore out --ignore tmp --exec 'jsdoc -c jsdoc.conf.json -t node_modules/ink-docstrap/template -r -p src'"
  },
  "main": "index.js",
  "repository": "https://github.com/wisnuc/wisnuc-drop.git",
  "author": "JianJin Wu <mosaic101@foxmail.com>",
  "license": "MIT",
  "merge_logs": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.6",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "express-socket.io-session": "^1.3.2",
    "formidable": "^1.1.1",
    "fundebug-nodejs": "^0.0.8",
    "getconfig": "^3.1.0",
    "ioredis": "^3.1.4",
    "joi": "^10.4.2",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.1",
    "moment": "^2.18.1",
    "mongoose": "^4.11.7",
    "mysql": "^2.13.0",
    "qcloud-weapp-server-sdk": "^0.8.2",
    "request": "^2.81.0",
    "request-json": "^0.6.2",
    "request-promise": "^4.2.1",
    "save": "^2.3.1",
    "sequelize": "^3.30.4",
    "socket.io": "^2.0.3",
    "ursa": "^0.9.4",
    "uuid": "^3.0.1",
    "wafer-node-session": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "gulp": "^3.9.1",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "sinon": "^2.3.8",
    "supertest": "^3.0.0",
    "socket.io-client": "^2.0.3"
  },
  "engines": {
    "node": ">=7.6.0"
  }
}
